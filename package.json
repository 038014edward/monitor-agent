{
  "name": "monitor-agent",
  "productName": "Monitor Agent",
  "version": "2.1.0-legacy",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf out node_modules/.cache",
    "clean:full": "rimraf out node_modules/.cache && node -e \"const fs=require('fs'); const path=require('path'); const cacheDir=path.join(process.env.LOCALAPPDATA||process.env.HOME, 'electron', 'Cache'); try{fs.rmSync(cacheDir, {recursive:true, force:true})}catch(e){}\"",
    "package": "npm run clean && electron-forge package",
    "make": "npm run clean && electron-forge make",
    "make:full": "npm run clean:full && electron-forge make"
  },
  "keywords": [],
  "author": "CMUBH",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "electron": "^22.3.27",
    "rimraf": "^5.0.10"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0"
  }

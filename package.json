{
  "name": "monitor-agent",
  "productName": "Monitor Agent",
  "version": "2.1.1",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report",
    "clean:full": "rimraf out node_modules/.cache && node -e \"const fs=require('fs'); const path=require('path'); const cacheDir=path.join(process.env.LOCALAPPDATA||process.env.HOME, 'electron', 'Cache'); try{fs.rmSync(cacheDir, {recursive:true, force:true})}catch(e){}\"",
    "package": "npm run clean && electron-forge package",
    "make": "npm run clean && electron-forge make",
    "make:full": "npm run clean:full && electron-forge make"
  },
  "keywords": [],
  "author": "CMUBH",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.57.0",
    "electron": "^39.2.2",
    "rimraf": "^5.0.10"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0"
  }
}